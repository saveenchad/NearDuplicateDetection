{
  "compilerOptions": {
    "downlevelIteration": true,
    "esModuleInterop": true,
    "lib": ["DOM", "ES6", "ESNext"],
    "module": "ESNext",
    "moduleResolution": "node",
    "outDir": ".",
    "skipLibCheck": true,
    "sourceMap": true,
    "target": "ESNext",
    "typeRoots": ["node_modules/@types"],
  },
  "exclude": ["node_modules"],
  "files": [
    "./Document.ts",
    "./DupeDetector.ts",
    "./utils.ts"
  ]
}
